{
  "reductions": {
    "r1": { "driver": "S", "body": "\\epsilon" },
    "r2": { "driver": "B", "body": "AS" },
    "r3": { "driver": "B", "body": "ab" },
    "r4": { "driver": "A", "body": "ba" },
    "r5": { "driver": "S", "body": "BAS" },
    "r6": { "driver": "A", "body": "BSS" }
  },
  "first_follow_set": {
    "S": {
      "first": ["a", "b"],
      "follow": ["a", "b", "\\$"],
      "nullable": true
    },
    "B": {
      "first": ["a", "b"],
      "follow": ["a", "b", "\\$"],
      "nullable": false
    },
    "A": {
      "first": ["a", "b"],
      "follow": ["a", "b", "\\$"],
      "nullable": false
    }
  },
  "parsing_table": {
    "s0": { "a": ["s4", "r1"], "b": ["s5", "r1"], "\\$": ["r1"], "S": ["1"], "B": ["2"], "A": ["3"] },
    "s1": { "a": ["acc"], "b": ["acc"], "\\$": ["acc"] },
    "s2": { "a": ["s4", "r1"], "b": ["s5", "r1"], "\\$": ["r1"], "S": ["7"], "B": ["2"], "A": ["6"] },
    "s3": { "a": ["s4", "r1"], "b": ["s5", "r1"], "\\$": ["r1"], "S": ["8"], "B": ["2"], "A": ["3"] },
    "s4": { "b": ["s9"] },
    "s5": { "a": ["s10"] },
    "s6": { "a": ["s4", "r1"], "b": ["s5", "r1"], "\\$": ["r1"], "S": ["11"], "B": ["2"], "A": ["3"] },
    "s7": { "a": ["s4", "r1"], "b": ["s5", "r1"], "\\$": ["r1"], "S": ["12"], "B": ["2"], "A": ["3"] },
    "s8": { "a": ["r2"], "b": ["r2"], "\\$": ["r2"] },
    "s9": { "a": ["r3"], "b": ["r3"], "\\$": ["r3"] },
    "s10": { "a": ["r4"], "b": ["r4"], "\\$": ["r4"] },
    "s11": { "a": ["r5", "r2"], "b": ["r5", "r2"], "\\$": ["r5", "r2"] },
    "s12": { "a": ["r6"], "b": ["r6"], "\\$": ["r6"] }
  },
  "format": {
    "transform_reductions": {
      "r1": "r2",
      "r2": "r3",
      "r3": "r4",
      "r4": "r6",
      "r5": "r1",
      "r6": "r5"
    },
    "transform_states": {
      "s0": "s0",
      "s1": "s12",
      "s2": "s4",
      "s3": "s3",
      "s4": "s1",
      "s5": "s2",
      "s6": "s8",
      "s7": "s9",
      "s8": "s7",
      "s9": "s5",
      "s10": "s6",
      "s11": "s10",
      "s12": "s11"
    },
    "column_order": ["a", "b", "\\$", "A", "B", "S"]
  }
}
